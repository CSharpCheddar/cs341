<Window x:Class="NFLStats.LiveScores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Control="clr-namespace:NFLStats"
        mc:Ignorable="d"
        Title="Scores" Height="450" Width="450" ResizeMode="CanMinimize">
    <Grid x:Name="Grid">
        <ScrollViewer>
            <StackPanel x:Name="panel" HorizontalAlignment="Left" Height="2400" VerticalAlignment="Center" Width="796"/>
        </ScrollViewer>
    </Grid>
</Window>
